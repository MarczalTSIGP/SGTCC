<% if @examination_board.can_create_defense_minutes?(current_professor) %>
  <generate-defense-minutes
    :can-generate="<%= !@examination_board.all_evaluated? %>"
    url="<%= professors_examination_board_defense_minutes_path(@examination_board) %>" />
  </generate-defense-minutes>

  <generate-non-attendance-defense-minutes
    url="<%= professors_examination_board_non_attendance_defense_minutes_path(@examination_board) %>" />
  </generate-non-attendance-defense-minutes>
<% end %>
