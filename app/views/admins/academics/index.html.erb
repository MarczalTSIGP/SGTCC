<% provide(:page_title, t('breadcrumbs.academics.index')) %>
<% provide(:head_title, t('breadcrumbs.academics.index')) %>

<%= link_to(new_admins_academic_path, class: 'btn btn-outline-primary float-right') do %>
  <i class="fe fe-plus"></i>
  <%= t('breadcrumbs.academics.new') %>
<% end %>

<div class="table-striped">
  <table class="table card-table table-vcenter text-nowrap">
    <thead>
      <tr>
        <th><%= Academic.human_attribute_name('name') %></th>
        <th><%= Academic.human_attribute_name('email') %></th>
        <th><%= Academic.human_attribute_name('ra') %></th>
        <th><%= Academic.human_attribute_name('gender') %></th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%= render 'admins/academics/academic' %>
    </tbody>
  </table>
</div>

<br>

