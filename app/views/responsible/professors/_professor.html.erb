<tr>
  <td><%= professor.name %></td>
  <td><%= professor.email %></td>
  <td><%= professor.username %></td>
  <td><%= professor.created_at.strftime("%d/%m/%Y") %></td>
  <td>
    <%= link_to(responsible_professor_path(professor)) do %>
      <i class="fe fe-search" data-toggle="tooltip" title="Show"></i>
    <% end %>
    <%= link_to(edit_responsible_professor_path(professor)) do %>
      <i class="fe fe-edit" data-toggle="tooltip" title="Editar"></i>
    <% end %>
    <%= link_to responsible_professor_path(professor),
      method: :delete,
      data: { confirm: t('prompt.confirm') },
      class: 'destroy' do %>
      <i class="fe fe-trash" data-toggle="tooltip" title="Delete"></i>
    <% end %>
  </td>
</tr>
