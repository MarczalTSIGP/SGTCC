{
  "extends": ["eslint:recommended", "plugin:vue/recommended"],
  "rules": {
    "semi": "error",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "vue/no-dupe-keys": ["off", { "groups": ["data"] }],
    "vue/html-self-closing": [
      "error",
      {
        "html": {
          "void": "never",
          "normal": "always",
          "component": "always"
        }
      }
    ]
  }
}
